  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "export": "sapper export --entry '/ /404'",
    "postexport": "mv __sapper__/export/404/index.html __sapper__/export/404.html && rmdir __sapper__/export/404",
    "readyForDeploy": "npm run export && npm run postexport",
    "deploy": "npm run export && node ./scripts/gh-pages.js"
  },
